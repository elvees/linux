			clock-names = "baudclk", "apb_pclk";
			interrupts = <GIC_SPI 54 IRQ_TYPE_LEVEL_HIGH>;
			reg-shift = <2>;
			reg-io-width = <4>;
			status = "disabled";

			pinctrl-names = "default";
			pinctrl-0 = <&uart1_mux_data>;

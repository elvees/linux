		uart2: serial0@1650000 {
			compatible = "snps,dw-apb-uart";
			reg = <0 0x1650000 0 0x1000>;
			clocks = <&lsperiph0_clocks CLK_LSP0_UCG0_UART2
				  &lsperiph0_clocks CLK_LSP0_UCG0_SYS>;
			clock-names = "baudclk", "apb_pclk";

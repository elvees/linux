					};

					data {
						groups = "sdmmc1_data";
						bias-pull-up;
					};

					cmd {
						pins = "SDMMC1_CMD";
						bias-pull-up;
					};
				};

				hsperiph_misc_default: hsperiph-misc-default {
					misc {
						groups = "hsperiph_misc";
						elvees,pad-enable;
					};
				};

				emac0_default: emac0-default {

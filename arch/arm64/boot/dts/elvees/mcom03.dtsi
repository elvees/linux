						groups = "nfc_io";
						function = "NAND";
					};
				};

				nand_voltage_33: nand-voltage-33 {
					volt {
						groups = "nfc_ctrl";
						power-source = <3300>;
					};
				};

				nand_voltage_18: nand-voltage-18 {
					volt {
						groups = "nfc_ctrl";
						power-source = <1800>;
					};
				};
			};
		};

		/*

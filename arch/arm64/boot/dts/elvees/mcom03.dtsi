					  <&lsperiph0_clocks CLK_LSP0_UCG0_SPI0>,
					  <&lsperiph0_clocks CLK_LSP0_UCG0_I2C0>,
					  <&lsperiph0_clocks CLK_LSP0_UCG0_GPIO0_DB>;

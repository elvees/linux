							 "qspi1_ss";
						bias-pull-up;
					};

					clk {
						pins = "QSPI1_SCLK";
						bias-pull-up;
					};
				};

				qspi1_default_18: qspi1-default-18 {
					groups = "qspi1_ctrl";
					power-source = <1800>;
				};

				qspi1_default_33: qspi1-default-33 {
					groups = "qspi1_ctrl";
					power-source = <3300>;
				};

				nand_default: nand-default {
					ctrl {
						groups = "nfc_ctrl";
